<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sid2019">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".APP.AlertasCulturaActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".APP.DatePickerActivity" android:screenOrientation="portrait" />
        <activity android:name=".APP.AlertasGlobaisActivity" android:screenOrientation="portrait" />
        <activity android:name=".APP.GlobaisActivity" android:screenOrientation="portrait" />
        <activity android:name=".APP.MainActivity" android:screenOrientation="portrait" />
        <activity android:name=".APP.LoginActivity" android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>